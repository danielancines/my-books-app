{
  "name": "my-books-app",
  "productName": "My Books App",
  "version": "1.0.0",
  "description": "App for search and save your books personal library",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=src/assets/icons/mac/mainIcon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=src/assets/icons/win/mainIcon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron Tutorial App\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=src/assets/icons/png/mainIcon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielancines/my-books-app.git"
  },
  "author": "Daniel Ancines",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danielancines/my-books-app/issues"
  },
  "homepage": "https://github.com/danielancines/my-books-app#readme",
  "devDependencies": {
    "electron": "^2.0.0"
  }
}
